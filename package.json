{
    "name": "healthznerbot",
    "version": "2.0.0",
    "description": "A discord bot to get periodically updates of the health status of your hetzner cloud machines.",
    "main": "main.js",
    "scripts": {
        "start": "node .",
        "dev": "nodemon .",
        "build": "tsc -b",
        "docker build": "docker build -t registry.raphaelbernhart.at/raphaelbernhart/healthznerbot:$npm_package_version .",
        "docker push": "docker push registry.raphaelbernhart.at/raphaelbernhart/healthznerbot:$npm_package_version"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/raphaelbernhart/healthznerbot.git"
    },
    "keywords": [
        "discord",
        "bot",
        "typescript",
        "health-check"
    ],
    "author": "Raphael Bernhart",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/raphaelbernhart/healthznerbot/issues"
    },
    "homepage": "https://github.com/raphaelbernhart/healthznerbot#readme",
    "dependencies": {
        "axios": "^0.25.0",
        "consola": "^2.15.3",
        "discord.js": "14.11.0",
        "dotenv": "^14.3.2",
        "hcloud-js": "^1.4.1"
    },
    "devDependencies": {
        "@types/dotenv": "8.2.0",
        "@types/node": "16.11.21",
        "@types/ws": "8.2.2",
        "nodemon": "2.0.15",
        "ts-node": "^10.9.1",
        "typescript": "4.5.5"
    }
}